#include <iostream>
#include <cctype>
#include <string>
using namespace std;
bool KTNA(char c){
   c=tolower(c); 
   return (c == 'a' || c == 'e' || c == 'i' || c == 'o' || c == 'u');
}
int main(){
   string b;
   cout<<"nhap chuoi ky tu b:";
   getline(cin,b);
   int count=0;
   for(char c : b){
    if(isalpha(c) && !KTNA(c)){
     count++;
 }
}
cout<<"cac chuoi ky tu khong phai la nguyen am la:"<<count<<endl;
return 0;
}
   
