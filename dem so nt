#include <iostream>
using namespace std;
bool KTSNT(int x) {
    if (x < 2) return false;
    if (x == 2) return true;
    if (x % 2 == 0) return false;  // Loại bỏ số chẵn ngay

    for (int i = 3; i * i <= x; i += 2) {  // i <= sqrt(x), chỉ xét lẻ
        if (x % i == 0)
            return false;
    }
    return true;
}
int main(){
    int n;
    cout<<"nhap so nguyen";
    cin>>n;
    if(n<=0){
        cout<<"nhap lai"<<endl;
        return 0;
    }
    int dem=0;
    int so=2;//bat dau tu 2
    while(dem<n){
        if(KTSNT(so)){
            cout<<so<<" ";
            dem ++;
        }
        so++;
    }
 cout<<endl;
    return 0;

}
