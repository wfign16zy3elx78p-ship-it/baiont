#include <iostream>
using namespace std;
void nhapmang(int a[],int n){
    for(int i=0;i<n;i++){
        cout<<"nhap vao phan tu ["<<i<<"]:";
        cin>>a[i];
    }
}
void xuatmang(int a[],int n){
    for(int i=0;i<n;i++)
    cout<<a[i];
}
int main(){
    int n;
    int a[100];
    cout<<"nhap so n";
    cin>>n;
    nhapmang(a,n);
    cout<<"xuat mang"<<endl;
    xuatmang(a,n);
    return 0;
}
